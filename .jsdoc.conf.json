{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "readme": "./README.md",
        "verbose": true,
        "theme_opts": {
            "theme": "dark"
        }
    },
    "source": {
        "include": ["Managers","Structures", "Util", "typings", "index.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}